    filters:
      proposta.flg_contrato_ativo: 'Yes'
      proposta.flg_contrato_cedido: 'Yes'
      financeiro.ds_titulo_status: ATIVO
    sorts: [financeiro.data_vencimento_date desc]
    limit: 500
    dynamic_fields: [{category: dimension, expression: 'if(${proposta.id_fundo_investimento}=5,"Itau","Pravaler")',
