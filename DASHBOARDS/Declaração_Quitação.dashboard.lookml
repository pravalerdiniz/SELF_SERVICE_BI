      proposta.semestre_financiado: Semestre
      financeiro.data_vencimento_date: Vencimento Boleto
      financeiro.data_pagamento_date: Data pagamento
      proposta.aluno_nome: Nome Aluno
      proposta.cpf_aluno: CPF Aluno
      financeiro.sum_boleto: Valor boleto
      financeiro.sum_pago: Valor Pago
    series_cell_visualizations:
      financeiro.sum_boleto:
        is_active: false
    series_text_format:
      tipo_de_aluno:
        align: center
      financeiro.id_contrato:
        align: center
      proposta.semestre_financiado:
        align: center
      financeiro.data_vencimento_date:
        align: center
      proposta.aluno_nome:
        align: center
      proposta.cpf_aluno:
