        dimension: tipo_de_aluno, _kind_hint: dimension, _type_hint: string}]
    show_view_names: false
    show_row_numbers: false
    transpose: false
    truncate_text: true
    hide_totals: false
    hide_row_totals: false
    size_to_fit: true
    table_theme: white
    limit_displayed_rows: false
    enable_conditional_formatting: false
    header_text_alignment: center
    header_font_size: '12'
    rows_font_size: '12'
    conditional_formatting_include_totals: false
    conditional_formatting_include_nulls: false
    show_sql_query_menu_options: false
    pinned_columns:
      "$$$_row_numbers_$$$": left
      proposta.aluno_nome: left
      proposta.cpf_aluno: left
    column_order: ["$$$_row_numbers_$$$", proposta.aluno_nome, proposta.cpf_aluno,
      tipo_de_aluno, financeiro.id_contrato, proposta.semestre_financiado, financeiro.data_vencimento_date,
      financeiro.sum_boleto, financeiro.data_pagamento_date, financeiro.sum_pago]
    show_totals: true
    show_row_totals: true
    series_labels:
